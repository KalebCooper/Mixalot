  pod 'LGButton'
  pod 'SwiftyJSON'
  pod "SwipeableTabBarController"

  target 'MixalotTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MixalotUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
